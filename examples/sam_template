[GlobalParams]
  {#-
      Create variables
  #}
  {%- set Mass_flow_rate = Tot_reactor_power / (He_cp * (He_outlet_temp - He_inlet_temp)) %}
  {%- set Channel_mass_flow_rate = Mass_flow_rate / num_cool_pins %}
  {#-
      Render the parameters
  #}
  global_init_P = {{ "{:.3e}".format(He_Pressure) }}
  global_init_V = {{ Channel_mass_flow_rate * Velocity_multiplication_factor / He_density * Coolant_channel_XS }}
  gravity = '-9.8 0 0'
